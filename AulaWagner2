#include <stdio.h>

int main() {
    int idade;
    
    printf("Digite sua idade: ");
    scanf("%d", &idade);
    
    if(idade <= 12) {
        printf("\nVocê é uma criança.");
    }else if(idade <= 17) {
        printf("\nVocê é um adolescente.");
    }else if(idade <= 64) {
        printf("\nVocê é um adulto.");
    }else{
        printf("\nVocê é um idoso.");
    }
    
    printf("\nA sua idade é %d", idade);
    
    return 0;
}
