// Pede 2 numeros ao usuario e imprime o maior deles

#include <stdio.h>

int main() {
    int num1;
    int num2;
    
    printf("Escreva um número: ");
    scanf("%d", &num1);
    
    printf("Escreva outro número: ");
    scanf("%d", &num2);
    
    
    if(num1 >= num2){
        printf("O número %d é maior", num1);
    }else{
        printf("O número %d é maior", num2);
    }
    

    return 0;
}
