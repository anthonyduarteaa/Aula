// Pede um numero e da a tabuada inteira dele

#include <stdio.h>

int main() {
    int numero;
    int i=1;
    
    printf("Digite um número para saber a tabuada dele: ");
    scanf("%d", &numero);
    
    printf("A tabuada do número %d é: \n", numero);
    
    while(i<=10){-
        printf("%dx%d=%d\n", numero, i, numero*i);
        i++;
    }
    
    
    
    

    return 0;
}
