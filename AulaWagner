#include <stdio.h>

int main() {
    int h = 252;
    int chute;
    
    printf("Tente adivinhar o número secreto: ");
    scanf("%d", &chute);
    
    if (chute == h) {
        printf("Acertou! \n");
    } else {
        printf("Você Errou! \n");
    }
    
    printf("O número secreto era %d", h);

    return 0;
}
