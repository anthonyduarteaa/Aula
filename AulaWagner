#include <stdio.h>

int main() {
    int h = 252;
    int chute;
    
    printf("Digite um número inteiro, positivo ou negativo: ");
    scanf("%d", &chute);
    
    if(chute == h){
        printf("Acertou! \n");
    }else{
        printf("Você Errou! \n");
    }
    printf("O Número Era %d", h);

    return 0;
}
